<script type="text/javascript">
    function setSignUpFormHeight() {
         var doc = $(window).height(); // document height
        var head = $(".modal-header").height(); // modal header height
        var footer = $(".modal-footer").height(); // modal footer height
        var modheight = doc - head - footer - 90; // 65 is extra margins and it will not effect the height of modal any way if not changed.
        $('.modal-body').css('height', modheight);
    }
    function openModal() {
       
        if ($('#txtEmail').val().length > 0) {
            $('<div class="modal-backdrop"></div>').appendTo(document.body);
            $('#myModal').slideDown('slow');
            $('#myModal').addClass('in');               
            setSignUpFormHeight();
            $('input[name=EMAIL]').val($('#txtEmail').val());                 
            $('#txtEmail').val('');
            return false;
        }
       else if ($('#txtEmailBanner').length > 0 && $('#txtEmailBanner').val().length > 0) {
            $('<div class="modal-backdrop"></div>').appendTo(document.body);
            $('#myModal').slideDown('slow');
            $('#myModal').addClass('in');
            setSignUpFormHeight();
            $('input[name=EMAIL]').val($('#txtEmailBanner').val());
            $('#txtEmailBanner').val('');
            return false;
        }
		else {
                    alert('Please Enter Email Address');
             }
    }
	
   
    function LazyLoad() {
        if (window.IntersectionObserver) {
            var config = { // If the image gets within 50px in the Y axis, start the download. 
                rootMargin: '255px 0px', threshold: 0.1
            };
           
            const imgObserver = new IntersectionObserver(showImage, config);			 
            const images = document.querySelectorAll('img.lazy')
            images.forEach(function (img) {
			
                imgObserver.observe(img);
				
				
            })
            function showImage(entries, observer) {
                for (var i = 0; i < entries.length; i++) {
                    var io = entries[i];
                    if (io.isIntersecting && io.intersectionRatio > 0) {
				
                        var image = io.target, src = image.getAttribute("data-src"), srcSet = image.getAttribute("data-srcset");
                          if($('img').hasClass("img-opacity"))
                          {
                            $('img').removeClass("img-opacity");
                          }
                        if (srcSet) { image.setAttribute("srcset", srcSet); }
                        if (src) { image.setAttribute("src", src); }
						
                        // Stop watching and load the image
                        observer.unobserve(io.target);
                    }
                }
            }
        }
        else {
            var lazyloadThrottleTimeout;
            const lazyloadImages = document.querySelectorAll(".lazy");
            function lazyload() {
                if (lazyloadThrottleTimeout) {
                    clearTimeout(lazyloadThrottleTimeout);
                }
                lazyloadThrottleTimeout = setTimeout(function () {
                    var scrollTop = window.pageYOffset;
                    for (let i = 0; i < lazyloadImages.length; i++) {
                        var img = lazyloadImages[i], src = img.getAttribute("data-src"), srcSet = img.getAttribute("data-srcset")
                        if (img.offsetTop < (window.innerHeight + scrollTop)) {
                            //console.log("lazy loading ", img)
                            if (srcSet) { img.setAttribute("srcset", srcSet); }
                            if (src) { img.setAttribute("src", src); }
                        }
                    }
                    if (lazyloadImages.length == 0) {
                        document.removeEventListener("scroll", lazyload);
                        window.removeEventListener("resize", lazyload);
                        window.removeEventListener("orientationChange", lazyload);
                    }
                }, 20);
            }

            document.addEventListener("scroll", lazyload);
            window.addEventListener("resize", lazyload);
            window.addEventListener("load", lazyload);
            window.addEventListener("orientationChange", lazyload);

        } 

    }
  
    
</script>



<!--end-->

