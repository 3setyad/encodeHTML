        $('#txtEmail').on('keypress', function (event) {
            if (event.which === 13) {
                if ($(this).val().length > 0) {
                     $('#myModal').slideDown('slow');
						$('#myModal').addClass('in');
						$('<div class="modal-backdrop"></div>').appendTo(document.body);
						setSignUpFormHeight();
