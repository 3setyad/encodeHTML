<script src="/new-branding/js/autocomplete/jquery-ui-1.12.1.js"></script>
<script>
var arrBasket = [];

    var product_id;
    $(document).ready(function () {
	LazyLoad();
        $(".userway userway_p5").addClass("hide");
        var isInIframe = (parent !== window);
        $(document).find('header').removeClass('header-height');
        if (isInIframe) {
            $('.cust-hidden').addClass('hidden');
        }
        else {
            $('.cust-hidden').removeClass('hidden');
        }
        $(function () {
            $('#txtSearch').autocomplete({
                source: function (request, response) {

                                             
                   var autocompleteUrl = '/autofill.asmx/GetProductName';
                    $.ajax({
                        type: 'POST',
                        url: autocompleteUrl,
                        data: "{'prefixText':'" + request.term + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: 'json',
                        success: function (json) {

                            var arr = [];
                            var i;
                         
                            response($.map(json, function (data, id) {


                                
                            
                             for (i = 0; i < data.length; i++) {


                                    arr.push({ "ProductName": data[i] });
                                }
                            

                            }));


                            response($.map(arr, function (data1, id) {


                                return {
                                    label: data1.ProductName,
                                    value: data1.ProductName
                                };


                            }));
                        },
                        error: function (xmlHttpRequest, textStatus, errorThrown) {
                            console.log('some error occured', textStatus, errorThrown);
                        }
                    });
                },
                minLength: 1,
                select: function (event, ui) {
                    $('#txtSearch').val(ui.item.label);
                    return false;
                }
            }).data("ui-autocomplete")._renderItem = function (ul, item) {

                
                return $("<li>").append("<a target='blank' href='/material-selector/?key=" + item.label + "' >" + item.label + "</a>").appendTo(ul);
            };
        });
		 getCartInfoforCollectionPage();
    });

  

    

    


    var add_status = false;

    function addtobasket(prdurl, _this) {
        product_id = _this.split('slcn-')[1];
        var obj = {};
        obj.PageUrl = prdurl;
        if (!$("#" + _this).hasClass("checked")) {
            $("#" + _this).addClass("checked");
        }
        else {
             add_status = true;
        }
        selection_status = $("#" + _this).hasClass("checked");
        obj.slcn_status = selection_status;
        $.ajax({
            type: 'POST',
            url: '/autofill.asmx/AddToSelectionCart',
            data: JSON.stringify(obj),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            beforeSend: function () {
                $('#info').html('');
                $('#info').hide();
                $('#imgLoader').show();
                $(".os-popup-body").hide();
                $('.tAreafooter textarea').val("");
                $('.noteLblfooter').text(""); 
            },
            complete: function () {
                getCartInfoforCollectionPage();
                console.log('complete;');              
                if (arrBasket.length > 0) {
                    $.each(arrBasket, function (index, item) {
                        console.log(item.ProductID.toString(), product_id);
                        if (item.ProductID.toString() !== product_id) {
                           
                            $('.tAreafooter').addClass("hidden");
                            if ($('.noteLblfooter ').text() !== null && $('.noteLblfooter ').text() !== '') {
                                $('.noteLblfooter').removeClass("hidden");
								 $('.noteLabelfooter').removeClass("hidden");
                            } else {
							console.log('hi');
                                $('.noteLblfooter ').addClass("hidden");
								 $('.noteLabelfooter ').addClass("hidden");
                            }
                            $('.spanNotefooter').text('Add a Note');
                           
                        }
                        else {
                            console.log(item.AdditionalNotes);
                            if (item.AdditionalNotes) {
                                console.log(1);
                                $('.tAreafooter').addClass("hidden");
                                $('.noteLblfooter').removeClass("hidden");
								$('.noteLabelfooter').removeClass("hidden");
                                $('.noteLblfooter ').text(item.AdditionalNotes);
                                $('.spanNotefooter').text('Edit Note');
                            }
                            else{
                                console.log(2);
                                $('.tAreafooter').addClass("hidden");
                                $('.noteLblfooter ').addClass("hidden");
								$('.noteLabelfooter ').addClass("hidden");
                                $('.spanNotefooter').text('Add a Note');
                            }
                             return false;
                        }
                    });
                }
                //else {
                //    $('.tAreafooter').addClass("hidden");
                //    $('.noteLblfooter ').addClass("hidden");
                //    $('.spanNotefooter').text('Add a Note');
                //}
                $('#imgLoader').hide(); $(".os-popup-body").show();
                //getCartInfo();
            },
            success: function (response) {
                getCartInfoforCollectionPage();
                console.log('success;'); 
                if (add_status) {
                    $('#osSlcnpopup p.info span.text').text("Item has been already added");
                    $.magnificPopup.open({
                        items: {
                            src: '#osSlcnpopup'
                        },
                        type: 'inline'
                    });
                    $("#" + _this).prop('checked', true);
                    $("#" + _this).addClass("checked");
                }
                else {
                    $('#osSlcnpopup p.info span.text').text("Item added successfully");
                    var favCount = parseInt($('#Header_lblFavCnt').text());
                    if ($('#Header_lblFavCnt').text() == null || $('#Header_lblFavCnt').text() == '') {
                        favCount = 0;
                    }
                    $('#Header_lblFavCnt').text(favCount + 1);
                    $.magnificPopup.open({
                        items: {
                            src: '#osSlcnpopup'
                        },
                        type: 'inline'
                    });
                    add_status = true;
                }
            }
        });
    }

    function getCartInfoforCollectionPage() {
       
        $.ajax({
            type: 'POST',
            url: '/autofill.asmx/GetSelectionCartInfo',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            complete: function () {
            },
            success: function (data) {
                if (data.d != "") {
                    var basket = JSON.parse(data.d);
                    arrBasket = basket;
                    $('.slcnCart .hearticon').each(function (idx, item) {
                        var itm = $(this);
                        if (basket.filter(e => e.ProductUrl === itm.attr('data-url')).length > 0) {
                            $('#cb-slcn-' + $(this).attr('data-id')).prop('checked', true);
                            $('#cb-slcn-' + $(this).attr('data-id')).addClass("checked");
                            add_status = true;
                        }
                        else {
                            add_status = false;
                        }
                    });
                  
                }
                else {

                }

            }
        });
    }
 var addnote ;
    $(document).on('click', ".ancNotefooter", function (e) {
        if ($('.tAreafooter').hasClass("hidden")) {
            if ($('.tAreafooter').is(':visible')) {
                $('.tAreafooter textarea').text($(this).parent().next().find('.noteLblfooter').text());
            }
            $('#osSlcnpopup .os-popup-body').addClass("pd-top");
            $('.tAreafooter').toggleClass('hidden');
            if ($('.tAreafooter').is(':visible')) {
                $(this).parent().next().find('.noteLblfooter').addClass('hidden');
            }
			addnote = $(this).parent().next().find('.noteLblfooter').text();
            if ($(this).parent().next().find('.noteLblfooter').length > 0) {
                $('.tAreafooter textarea').val($(this).parent().next().find('.noteLblfooter').text());
                $(this).parent().next().find('.noteLblfooter').removeClass('hidden');
                 $(this).parent().next().find('.noteLabelfooter').removeClass('hidden');
                if ($('.tAreafooter').is(':visible')) {
                    $(this).parent().next().find('.noteLblfooter').addClass('hidden');
                    $(this).parent().next().find('.noteLabelfooter').addClass('hidden');
                }
            }
        }
    });
	$(document).on('keyup', 'textarea', function () {
    $(this).parent().find('span').text(200 - $(this).val().length);
    if ($(this).val().length == 200)
        $(this).parent().find('i').css('color', 'red');
});

    $(document).on('click', 'a.SaveNotefooter', function () {
        var obj = {};
        obj.taVal = $(this).parent().parent().find('textarea').val();
        var _this = $(this);
        obj.tid = product_id;
            obj.GuestId = null;
        if (obj.taVal != null && obj.taVal != '') {
           console.log(obj);
            $.ajax({
                type: 'POST',
                url: '/autofill.asmx/updateSelectionCartInfo',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: JSON.stringify(obj),
                complete: function () {
                   
                },
                success: function (data) { 
				_this.parent().parent().parent().next().find('.noteLabelfooter').removeClass('hidden');
                    _this.parent().parent().parent().next().find('.noteLblfooter').removeClass('hidden').text(obj.taVal);
                    _this.parent().parent().parent().find('a.ancNotefooter span.spanNotefooter').text('Edit Note');
                    _this.parent().parent().parent().find('div.tAreafooter').addClass('hidden');
                      getCartInfoforCollectionPage();
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        }
        else {
            _this.parent().parent().parent().find('a.ancNotefooter span.spanNotefooter').text('Add Note');
            $('.noteLblfooter').addClass('hidden');
            $('.noteLblfooter').length = 0;
            $('.noteLblfooter').text('');
          // $('.noteLabelfooter').addClass('hidden');
              _this.parent().parent().parent().find('div.tAreafooter').addClass('hidden');
            //_this.parent().parent().parent().find('div.tArea').addClass('hidden');
        }
    });
